html {
  min-width: 320px;
}

html body {
  background-color: #ededed;
  font-family: Arial, Helvetica, sans-serif;
  width: 100%;
  overflow-x: hidden;
}

html .no-gutter {
  margin: 0 !important;
}

html .noPadding {
  padding: 0 !important;
}

@media only screen and (min-width: 993px) {
  html .noPadding-md-up {
    padding: 0 !important;
  }
}

@media only screen and (max-width: 992px) {
  html .noPadding-md-down {
    padding: 0 !important;
  }
}

html .noMargin {
  margin: 0 !important;
}

html .not-mb, html .mb {
  margin-bottom: 0;
}

html a {
  cursor: pointer;
}

html .text-uppercase {
  text-transform: uppercase;
}

html .strong {
  font-weight: bold !important;
}

html .left {
  float: left;
}

html .container {
  max-width: 1200px;
}

@media only screen and (min-width: 601px) {
  html .container {
    width: 90%;
  }
}

@media only screen and (min-width: 993px) {
  .md-up {
    display: none;
  }
}

@media only screen and (max-width: 992px) {
  .md-down {
    display: none;
  }
}

.fr-wrapper > div > a {
  width: 1px !important;
  height: 1px !important;
  overflow: hidden !important;
  background: white !important;
  float: right !important;
}

@media (max-width: 992px) {
  .flex-m {
    display: -webkit-box;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
    /* Stack on top */
  }
  .box-first {
    -webkit-box-ordinal-group: 3;
    order: 2;
    /* Go down, bring Box B up */
  }
  .box-last {
    -webkit-box-ordinal-group: 4;
    order: 3;
    /* Go down, bring Box B up */
  }
}

.page-content h2 {
  color: #989ea7;
  font-size: 25px;
  font-weight: bold;
}

@media only screen and (max-width: 992px) {
  .page-content h2 {
    text-align: center;
  }
}

.page-content h3 {
  color: #004681;
  font-size: 15px;
}

@media only screen and (max-width: 992px) {
  .page-content h3 {
    text-align: center;
  }
}

@media only screen and (max-width: 992px) {
  .padding, .row-p {
    padding-left: calc(20px - 10px) !important;
    padding-right: calc(20px - 10px) !important;
  }
  div[id*="slider-"] .padding, div[id*="slider-"] .row-p {
    padding-left: 20px !important;
    padding-right: 20px !important;
  }
}

.left-align-i {
  text-align: left !important;
}

.right-align-i {
  text-align: right !important;
}

.center-align-i {
  text-align: center !important;
}

.collapse + ul {
  display: none;
}

.titulo {
  font-size: 14px;
}

.titulo.gray {
  color: #646464;
}

.titulo.gray i {
  color: #ef7a3d;
}

.titulo.gray + ul {
  margin: 15px 0px 24px;
  padding: 0px;
  border: 0px;
  list-style-type: none;
}

.titulo.gray + ul li {
  font-size: 12px;
}

.titulo.gray + ul li a {
  color: #ef7a3d;
}

.titulo.gray + ul li a:hover {
  text-decoration: underline;
}

.text-orange, .description ul li, .description ul li a {
