</script> <script>
    var valido = true;
    function valida(campo) {
        var valor = $('#' + campo).val();
        var reg = null;
        var msj = null;
        if (valor.trim() === "") {
            msj = "Campo requerido.";
        } else if (campo === "correo") {
            reg = /^(([^<>()[\]\\.,;:\s@"]+(\.[^<>()[\]\\.,;:\s@"]+)*)|(".+"))@(([[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
            if (!reg.test(valor)) {
                msj = "Capture un correo vlido.";
            }
        } else if (campo === "nombre") {
            reg = /^[a-zA-Z\s]+$/;
            if (!reg.test(valor)) {
                msj = "Capture un nombre vlido.";
            }
        } else if (campo === "telefono") {
            if (/^[01]/.test(valor)) {
                msj = 'No es posible la captura si inicia en 1 o 0.';
            }
            if (!/^\d{10}$/.test(valor) && msj === null) {
                msj = "Capture un telfono vlido.";
            }
            if (/[0-9]*(1{5}|2{5}|3{5}|4{5}|5{5}|6{5}|7{5}|8{5}|9{5}|0{5})[0-9]*/.test(valor) && valor > 4) {
                msj = 'Inv\u00E1lido, 5 dig\u00EDtos iguales.';
            }
        } else if (campo === "codigoP") {
            if ($("#error" + campo).text() != "") {
                msj = "El C.P. no existe.";
            }
            reg = /^\d{5}$/;
            if (!reg.test(valor)) {
                msj = "Introduzca un C.P. de 5 dgitos.";
            }
        } else if (campo === "privacidad" || campo === "queEres") {
            let val = $(`input[name='${campo}']:checked`).val();
            val == undefined ? msj = "Campo requerido." : null;
        } else if (campo === "walls" || campo === "disaster") {
            let vl = $(`input[name=${campo}]:checked`).val();
            vl === undefined ? msj = "Campo requerido." : null;
            if (campo === "disaster" && vl === "PROTECCION") {
                let vlake = $(`input[name=lake]:checked`).val();
                let vbeach = $(`input[name=beach]:checked`).val();
                if (vlake === undefined) {
                    valido = false;
                    $("#errorlake").show();
                    $("#errorlake").text("Campo requerido.");
                }
                if (vbeach === undefined) {
                    valido = false;
                    $("#errorbeach").show();
                    $("#errorbeach").text("Campo requerido.");
                }
            }
        }
        if (msj) {
            valido = false;
            $("#error" + campo).show();
            $("#error" + campo).text(msj);
        } else {
            $("#error" + campo).hide();
            $("#error" + campo).text("");
        }
        return valido;
    }
    var intervalHogar = setInterval(function() {
        console.log("interval");
        hogarClick();
    }, 1000);
    var hogarClick = function() {
        $(document).ready(function() {
            let ua = navigator.userAgent.toLowerCase();
            let isAndroid = ua.indexOf("android") > -1;
            $(".errorMsj").hide();
            $("#btnCotizar").on('click', function() {
                $("#modalHogar").addClass("muestraLoad");
                valido = true;
                var data = ["nombre", "correo", "telefono", "codigoP", "walls", "valor-inmueble", "disaster", "privacidad", "queEres"];
                var camposEnvio = ["queEres", "nombre", "correo", "telefono", "codigoP", "walls", "valor-inmueble", "disaster"];
                for (var i = 0; i < data.length; i++) {
                    if (!valida(data[i])) {
                        valido = false;
                    }
                }
                if ($('input[name=walls]:checked').val() === "NOCONCRETO") {
                    $("#containerConcreto").addClass("muestraLoad");
                    valido = false;
                }
                if (valido) {
                    $.get(`https://us-central1-gnp-portalcorporativo-pro.cloudfunctions.net/checkcp?cp=${$("#codigoP").val()}`)
                        .done(function() {
                            $.get(`https://us-central1-gnp-portalcorporativo-pro.cloudfunctions.net/check_code?cp=${$("#codigoP").val()}`)
                                .done(function() {
                                    $("#errorcodigoP").hide();
