  color: #183d7b;
  margin: 0;
  padding: 0px 20px;
}

#slider-menu .footer-content button {
  border: none;
  box-shadow: none;
  background-color: #183d7b;
  margin: 0px;
  display: block;
  border-radius: 5px;
  position: relative;
  padding: 3px 8px;
  font-size: 12px;
  margin: 10px 20px 10px 20px;
  color: white;
}

#slider-menu .persiana-active .desc-content {
  width: 25%;
}

#slider-menu .persiana-active .head-content button {
  opacity: 0;
  pointer-events: none;
}

#slider-menu .persiana-active .footer-content {
  opacity: 0;
  pointer-events: none;
}

#slider-menu .persiana-unactive .head-content, #slider-menu .persiana-unactive .footer-content {
  opacity: 0;
  pointer-events: none;
}

