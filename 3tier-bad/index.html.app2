function templateGenerate(resolution , dataSlider){
    return new Promise(function(resolve, reject) {
        $.each(dataSlider, function(index, value) {
            if (index == 'resolution') {
                dataSlider["resolution"] = resolution;
            }
        });
        
        var idSeection = $('[data-slider]').attr('id');
        if (idSeection === undefined) {
            resolve(false);
        }
    
        var htmlBM = timer("HTML set");
        console.log(dataSlider);
        console.log(window.navigator.userAgent);
        if (window.navigator.userAgent.indexOf("Mac OS X") != -1 && window.navigator.appVersion.indexOf("Chrome") == -1 && window.navigator.appVersion.indexOf("iPhone") == -1 && window.navigator.appVersion.indexOf("iPad") == -1) {
            $.post("https://us-central1-gnp-portalcorporativo-qa.cloudfunctions.net/compileHome", { "template": $("#slider-template-tmpl").text(), "data": dataSlider }, function(data) {
                document.getElementById(idSeection).innerHTML = data.result;
                htmlBM.stop();
                
                 if (resolution == 'mobile') {
                    $("div[data-module='module']").each(function(index, value) {
                        var _this = $(this);
                        var htmlConteiner = _this.parent().html();
                        $('.viewMobile').append(htmlConteiner);
                    });
                    
                    $(".item-body div[data-module='module']").parent().html('');
                    
                    $(".slides.horizontal li").each(function(index) {
                        $(this).attr({'data-item': index, 'id': 'id_' + index});
                    });
                    slider_mobile();
                }
                
                initialization_form(resolution);
                
                resolve(false);
            });
        } else {
            document.getElementById(idSeection).innerHTML = tmpl("slider-template-tmpl", dataSlider);
            htmlBM.stop();
                
            if (resolution == 'mobile') {
                $("div[data-module='module']").each(function(index, value) {
                    var _this = $(this);
                    var htmlConteiner = _this.parent().html();
                    $('.viewMobile').append(htmlConteiner);
                });
                
                $(".item-body div[data-module='module']").parent().html('');
                
                $(".slides.horizontal li").each(function(index) {
                    $(this).attr({'data-item': index, 'id': 'id_' + index});
                });
                slider_mobile();
            }
            
            initialization_form(resolution);
            
            resolve(false);
        }
    });
}

window.sliderMobileOpen = false;

function slider_mobile(){
    $('a[href*="#"]').click(function() {
        var url = $(this).attr('href');
        if(url.match('#')){
            var idSeection= $('div[id^="slider"]').attr('id');
            $('#'+idSeection).find('div[data-module="module"]').addClass('md-down');
            $('#'+$(this).attr('href').split('#')[1]).removeClass('md-down');
            if (location.hostname == this.hostname) {
                var $target = $(this.hash);
                $target = $target.length && $target || $('[name=' + this.hash.slice(1) +']');
                if ($target.length) {
                    
                    //PAUSAMOS SLIDER
                    $(".slider").trigger("sliderPause");
                    document.location.href = "#" + $(this).closest('.item').find("h2.title a").text();
                    
                    $("div.viewMobile i.material-icons.expand-less").off().click(function() {
                        $('.slider').trigger("sliderNext").trigger("sliderStart");
                    });
                    
                    var targetOffset = $target.offset().top;
                    
                    if($(this).hasClass("btn-orange") && $target[0].id == "section-carro") {
                        targetOffset = $(".responsive-img").offset().top;
                    }
                    if($(this).hasClass("btn-orange") && $target[0].id == "section-hogar") {
                       targetOffset = $(".formulario-cotiza-casamas").offset().top;
                    }
                    
                    $('html,body').animate({scrollTop: targetOffset});
                    sliderMobileOpen = $(this).closest("li").data().item;
                    
                    return false;
                }
            }
        }
    });
}

function slider_mobile_open(id) {
    try {
        var idSlider = $('div[id^="slider"]').attr('id');
        var botones = $("#"+idSlider+" .slides.horizontal li div.item-footer-buttom a.btn-small");
        
        if(window.btnOrange) {
            botones = $("#"+idSlider+" .slides.horizontal li div.item-header a.btn-orange");
        }
        
        botones[id].click();
        setTimeout(function(){ persiana.resize(); 
            $(".slider").trigger("sliderPause");
            var event = jQuery.Event("sliderGoTo"); event.id = id;
            $(".slider").trigger(event);
        }, 500);
        
    } catch(error) {
        console.log("open default slider", error);
    }
}

function initialization_form(resolution){
    //PHB $('select').material_select();

    $('.tooltipped').tooltip({
        delay: 50,
        position: resolution == 'mobile'?'bottom':'right',
        html:true,
    });
    
    $('.material-tooltip').addClass('type-1');
    $("li.item .item-header").removeAttr("style");
    
    if(resolution == "desktop") {
        persiana.setElements();
        persiana.initial();
        
        //CODIGO PARA ABRIR PERSIANA POR URL
        if(window.openByUrl !== false) {
            var data = $("li.item .item-header h2.title a:contains('"+window.openByUrl+"')").closest('.item').data();
            if(data && data.item != undefined) {
                window.sliderMobileOpen = data.item;
            }
            window.openByUrl == false;
        }
        
        if(window.sliderMobileOpen !== false) {
            console.log("persiana.open");
            persiana.inProcess = true;
            persiana.open(window.sliderMobileOpen);
            //setTimeout(function(){ persiana.resize(); }, 500);
        }
    }
    
    if(resolution == "mobile") {
        
        //CODIGO PARA ABRIR PERSIANA POR URL
        if(window.openByUrl !== false) {
            var data = $("li.item .item-header h2.title a:contains('"+window.openByUrl+"')").closest('.item').data();
            if(data && data.item != undefined) {
                persiana.isOpen = true; persiana.isTabs = data.item;
            }
            window.openByUrl == false;
        }
        
        if(persiana.isOpen && persiana.isTabs !== false) {
            window.sliderMobileOpen = persiana.isTabs;
            setTimeout(slider_mobile_open(persiana.isTabs), 500);
        }
        
        persiana.setElements();
    }

    return false;
}

