var isFlicker = false;

function createFooterFlickity(){
    $('.footerCarousel').flickity({
        accessibility:   true,
        adaptiveHeight:  true,
        autoPlay:        false,
        cellAlign:       'center',
        cellSelector:    '.footerCarousel-links',
        draggable:       true,
        dragThreshold:   1,
        freeScroll:      false,
        friction:        0.2,
        groupCells:      true,
        initialIndex:    0,
        lazyLoad:        true,
        prevNextButtons: false,
        pageDots:        false,
        resize:          true,
        rightToLeft:     false,
        setGallerySize:  false,
        watchCSS:        false,
        wrapAround:      false
    });

    return isFlicker = true;
};

function isFlick( hasFlicker ){
    if (window.matchMedia('(max-width: 992px)').matches) {
        //Mobile..
        if( hasFlicker == false) {
            if(typeof jQuery().flickity == "undefined") {
                $.getScript("https://unpkg.com/flickity@2.0.10/dist/flickity.pkgd.min.js", function( data, textStatus, jqxhr ) {
                    hasFlicker = createFooterFlickity();
                });
            } else {
                hasFlicker = createFooterFlickity();
            }
        }
    
    } else if(window.matchMedia('(min-width: 993px)').matches) {
        //Desktop..
        if( hasFlicker == true) {
            $('.footerCarousel').flickity('destroy');
            hasFlicker = false;
        }
    }

    return hasFlicker;
}

$( document ).ready(function() {
    isFlicker = isFlick( isFlicker );
});

$( window ).resize(function() {
    isFlicker = isFlick( isFlicker );
});

