#soy-cliente #ntfMain .ntf-overlayer {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  overflow: hidden;
  pointer-events: none;
  z-index: 9000;
}

#soy-cliente .ntf-overlayer:after {
  content: '';
  display: block;
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.6);
  opacity: 0;
  will-change: opacity;
  pointer-events: none;
  transition: opacity 0.3s cubic-bezier(0, 0, 0.3, 1);
}

#soy-cliente .ntf-overlayer-opened.ntf-overlayer:after {
  opacity: 1;
  pointer-events: auto;
}

#soy-cliente #ntfMain .ntf-overlayer-opened .ntf-contenedor {
  pointer-events: auto;
  -webkit-transform: none;
  transform: none;
  transition: -webkit-transform 400ms cubic-bezier(0.46, -0.04, 0.11, 1);
  transition: transform 400ms cubic-bezier(0.46, -0.04, 0.11, 1);
  transition: transform 400ms cubic-bezier(0.46, -0.04, 0.11, 1), -webkit-transform 400ms cubic-bezier(0.46, -0.04, 0.11, 1);
}

