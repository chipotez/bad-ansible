  width: 350px;
  position: absolute;
  top: 0;
  z-index: -1;
  left: 0;
  opacity: 1;
  transition: opacity .2s linear;
}

#slider-menu .desc-content .head-content button {
  border: none;
  box-shadow: none;
  background-color: #ff6f20;
  margin: 10px auto;
  display: block;
  position: relative;
  font-size: 12px;
  border-radius: 5px;
  color: white;
  padding: 3px 8px;
  transition: opacity .1s linear;
}

#slider-menu .footer-content {
  position: absolute;
  bottom: 0;
  width: 100%;
  background-color: rgba(255, 255, 255, 0.6);
  transition: opacity .2s linear;
}

