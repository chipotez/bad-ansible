var persiana = {

    slider: null,
    items: null,
    childs: 0,

    cardOpen: 0,
    cardCollaps: 0,
    cardInfo: 0,
    cardImg: 0,

    ms: 400,
    isTabs: false,
    isOpen: false,
    inProcess: false,

    ////////////////// ELEMENTOS ///////////////////
    setElements: function() {
        this.isTabs = false;
        this.isOpen = false;
        this.slider = document.getElementsByClassName('slider')[0];
        this.items = document.getElementsByClassName('item');
        this.childs = document.getElementsByClassName('item').length;
        this.itemHeader = document.getElementsByClassName('item-header');
    },
    ////////////////////////////////////////////////

    /////////////////// MEDIDAS ////////////////////
    getSize: function() {
        var numTabs = {
            2 : { max : 95, min : 3, img: 0.24 },
            3 : { max : 90, min : 7, img: 0.24 },
            4 : { max : 85, min : 13, img: 0.2929 },
            5 : { max : 80, min : 18, img: 0.248 }
        };

        this.cardInfo = numTabs[this.childs].max;
        this.cardOpen = 100 / this.childs;
        this.cardCollaps = numTabs[this.childs].min / (this.childs - 1);
        this.cardImg = numTabs[this.childs].img;
    },
    ////////////////////////////////////////////////

    /////////////////// EVENTOS ////////////////////
    run: function() {
        $('.target-click').click(function(ev) {
            if(persiana.inProcess == false) {
                persiana.inProcess = true;
                
                var $tabItem = $(this).closest('.item');
                document.location.href = "#" + $tabItem.find("h2.title a").text();
                
                if(persiana.isOpen && $tabItem.hasClass('active')) {
                    persiana.close($tabItem.data().item);
                } else if(persiana.isOpen) {
                    persiana.swiping($tabItem.data().item);
                } else {
                    persiana.open($tabItem.data().item);
                }
            }
        });
    },
    ////////////////////////////////////////////////

    ////////////////// INICIALIZA //////////////////
    initial: function() {
        $('.target-click').off();
        $('.slider > ul .item').css({'transition':'width 450ms ease-in-out'});
        $('.item-header').css({'transition':'width 0ms ease'});
        $('.slider').find('.item').each(function(index) {
            $(this).attr({'data-item': index, 'id': 'id_' + index});
        });
        
        this.setElements();
        if(this.items != null && this.childs > 0) {
            this.getSize();
            this.run();	
        }
        
        console.log("persiana.initial");
    },
    ////////////////////////////////////////////////

    //////////////////// ABRIR /////////////////////
    open: function(idTab) {
        this.itemHeader[idTab].style.transitionDuration = "0ms";
        
        var hrz = document.getElementsByClassName("horizontal")[0].offsetWidth;
        var wdt = ((this.cardInfo / 100) * hrz) * this.cardImg  + 'px';
        
        this.items[idTab].getElementsByClassName('item-header')[0].style.width = wdt;
        this.slider.className = this.slider.className + ' active';
        
        for(var i = 0; i < this.childs; i++) {
            if(i != idTab) {
                this.items[i].style.width =  this.cardCollaps + '%';
            }
        }
        
        this.items[idTab].style.width =  this.cardInfo + '%';
        
        setTimeout( function() {
            persiana.items[idTab].className = 'item active';
            persiana.inProcess = false;
        }, this.ms);

        this.isTabs = idTab;
        this.isOpen = true;
    },
    ////////////////////////////////////////////////

    //////////////////// CERRAR ////////////////////
    close: function(idTab) {
        this.items[idTab].className = 'item';
        
        for(var i = 0; i < this.childs; i++){
            this.items[i].style.width =  this.cardOpen + '%';
            this.itemHeader[i].style.transitionDuration = "100ms";
        }
        
        setTimeout( function() {
            persiana.items[idTab].getElementsByClassName('item-header')[0].style.width = null;
            persiana.slider.className = persiana.slider.className.replace(/active/g, '');
            persiana.inProcess = false;
        }, this.ms);

        this.isTabs = false;
        this.isOpen = false;
    },
    ////////////////////////////////////////////////

    /////////////////// DESLIZAR ///////////////////
    swiping: function(idTab) {
        var width = this.items[this.isTabs].getElementsByClassName('item-header')[0].style.width;
        
        if(idTab > this.isTabs) {
            this.itemHeader[idTab].style.maxWidth = width;
            this.itemHeader[idTab].style.transitionDuration = "450ms";
        }
        
        this.items[this.isTabs].getElementsByClassName('item-header')[0].style.maxWidth = width
        this.items[this.isTabs].getElementsByClassName('item-header')[0].style.width = null;
        
        this.items[this.isTabs].className = 'item';
        this.items[this.isTabs].style.width =  this.cardCollaps + '%';
        
        //this.items[idTab].getElementsByClassName('item-header')[0].style.width = width;
        this.itemHeader[idTab].style.maxWidth = width;
        this.items[idTab].style.width =  this.cardInfo + '%';
        
        setTimeout( function() {
            //persiana.items[persiana.isTabs].getElementsByClassName('item-header')[0].style.width = null;
            //persiana.items[persiana.isTabs].getElementsByClassName('item-header')[0].style.maxWidth = null;
            persiana.itemHeader[persiana.isTabs].style.maxWidth = null;
            
            persiana.itemHeader[idTab].style.width = width;
            persiana.itemHeader[idTab].style.maxWidth = null;
            persiana.itemHeader[idTab].style.transitionDuration = "0ms";
            
            persiana.items[idTab].className = 'item active';
            persiana.isTabs = idTab;
            persiana.inProcess = false;
        }, this.ms);
        
        this.isOpen = true;
    },
    ////////////////////////////////////////////////
    
    //////////////////// RESIZE ////////////////////
    resize: function() {
        if(this.isOpen) {
            this.items[this.isTabs].getElementsByClassName('item-header')[0].style.width = "24%";
            
            setTimeout( function() {
                var width = persiana.items[persiana.isTabs].getElementsByClassName('item-header')[0].offsetWidth
                persiana.items[persiana.isTabs].getElementsByClassName('item-header')[0].style.width = width + "px";
            }, this.ms);
        }
    }
    ////////////////////////////////////////////////
}

