                                    $('input[name=disaster]:checked').val() === 'PROTECCION' ? url = url + `&playa=${$('input[name=beach]:checked').val()}&lago=${$('input[name=lake]:checked').val()}&desastres=${$('input[name=disaster]:checked').val()}` : null;
                                    $.get(`https://us-central1-gnp-portalcorporativo-pro.cloudfunctions.net/cotizarhogarversatil?${url}`)
                                        .done(function(result) {
                                            result.telefono = {
                                                value: $("#telefono").val()
                                            };
                                            var res = encodeURI(JSON.stringify(result, null, 2));
                                            window.location = `https://www.gnp.com.mx/personas/hogar/seguro-casa-habitacion/cotizador-casa-mas/?res=${res}`;
                                        });
                                }).fail(function() {
                                    $("#containerConcreto").addClass("muestraLoad");
                                    $("#modalHogar").removeClass("muestraLoad");
