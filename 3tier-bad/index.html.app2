  color: #4B4B4B;
  position: relative;
  padding-left: 20px;
}

.ticker-content a:hover {
  color: #4B4B4B;
}

.ticker-content a:before {
  content: '';
  display: block;
  background: url(../../images/site/ico_alerta.jpg) no-repeat center;
  background-size: cover;
  height: 15px;
  width: 15px;
  position: absolute;
  top: 0;
  left: 0;
}

