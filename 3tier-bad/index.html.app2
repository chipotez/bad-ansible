                        }).fail(function() {
                            $("#modalHogar").removeClass("muestraLoad");
                            $("#errorcodigoP").show();
                            $("#errorcodigoP").text("El C.P. no existe.");
                        });
                } else {
                    $("#modalHogar").removeClass("muestraLoad");
                }
            });
            $("#closeContainerConcreto").on('click', () => {
                $("#containerConcreto").removeClass("muestraLoad")
            })
            $(":input").keyup(function(event) {
                if (event.target.name == "telefono") {
                    let errorMessage = "";
                    if (/[0-9]*(1{5}|2{5}|3{5}|4{5}|5{5}|6{5}|7{5}|8{5}|9{5}|0{5})[0-9]*/.test(event.target.value) && event.target.value.length > 4) {
                        errorMessage = 'Inv\u00E1lido, 5 dig\u00EDtos iguales.';
                        $("#error" + event.target.name).show();
                    } else {
                        $("#error" + event.target.name).hide();
                    }
                    $("#error" + event.target.name).text(errorMessage);
                } else {
                    $("#error" + event.target.name).hide();
                    $("#error" + event.target.name).text("");
                }
            });
            function checkNombre(event) {
                var values = this.value.split("");
                var finalValue = "";
                values.forEach((value, index) => {
                    if (/^[a-zA-Z\s]+$/.test(value)) {
                        finalValue += value;
                    }
                });
                this.value = finalValue;
            }
            document.getElementById("nombre").addEventListener("keyup", checkNombre);
            $("#telefono").keyup(function(e) {
                if (e.target.value.length <= 4) {
                    $("#errortelefono").hide();
                    $("#errortelefono").text("");
                }
                this.value = this.value.replace(/[^0-9]+/g, "");
            });
            function checkCp(event) {
                this.value = this.value.replace(/[^0-9]+/g, "");
                var reg = /^\d{5}$/;
                var msj = null;
                if (!reg.test(this.value)) {
                    msj = "Introduzca un C.P. de 5 dgitos.";
                }
                if (msj) {
                    $("#errorcodigoP").show();
                    $("#errorcodigoP").text(msj);
                }
            }
            document.getElementById("codigoP").addEventListener("keyup", checkCp);
            document.getElementById("codigoP").addEventListener("blur", function() {
                $.get(`https://us-central1-gnp-portalcorporativo-pro.cloudfunctions.net/checkcp?cp=${$("#codigoP").val()}`)
                    .done(function() {}).fail(function() {
                        var valor = $('#codigoP').val();
                        if (valor === "") {
                            $("#errorcodigoP").show();
                            $("#errorcodigoP").text("Campo requerido.");
                        } else {
                            $("#errorcodigoP").show();
                            $("#errorcodigoP").text("El C.P. no existe.");
                        }
                    });
            });
            $("#privacidad").change(function(event) {
                $("#errorprivacidad").hide();
                $("#errorprivacidad").text("");
            });
            $("input[type=radio]").change(function(event) {
                $("#error" + event.target.name).hide();
                $("#error" + event.target.name).text("");
                if (event.target.name === "disaster") {
                    let vl = $(`input[name=${event.target.name}]:checked`).val();
                    if (vl === "NOPROTECCION") {
                        $("#errorlake").hide();
                        $("#errorbeach").hide();
                        $("input[name=lake]").attr('disabled', 'disabled');
                        $("input[name=beach]").attr('disabled', 'disabled');
                        $("input[name=lake]").prop('checked', false);
                        $("input[name=beach]").prop('checked', false);
                        $("#dependientesProteccion").addClass("disabledDisaster");
                    } else if (vl === "PROTECCION") {
                        $("input[name=lake]").removeAttr('disabled');
                        $("input[name=beach]").removeAttr('disabled');
                        $("#dependientesProteccion").removeClass("disabledDisaster");
                    }
                }
            });
            $("select").change(function(event) {
                $("#error" + event.target.name).hide();
                $("#error" + event.target.name).text("");
            });
        });
        if ($("#btnCotizar").length > 0) {
            clearInterval(intervalHogar);
        }
    };
