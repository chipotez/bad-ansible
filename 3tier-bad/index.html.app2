  transition: opacity .1s ease-in-out, -webkit-transform .7s ease-in-out;
  transition: opacity .1s ease-in-out, transform .7s ease-in-out;
  transition: opacity .1s ease-in-out, transform .7s ease-in-out, -webkit-transform .7s ease-in-out;
